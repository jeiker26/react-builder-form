{
  "collectCoverage": true,
  "collectCoverageFrom": [
    "<rootDir>/src/**/*.{js,jsx}",
    "!<rootDir>/src/setupTests.js",
    "!<rootDir>/src/**/index.js",
    "!<rootDir>/node_modules/"
  ],
  "coverageDirectory": "coverage",
  "coverageThreshold": {
    "global": {
      "branches": 100,
      "functions": 100,
      "lines": 100,
      "statements": 100
    }
  },
  "notify": true,
  "roots": [
    "<rootDir>/src"
  ],
  "setupFilesAfterEnv": ["<rootDir>/src/setupTests.js"],
  "testURL": "http://localhost",
  "transform": {
    "^.+\\.(js|jsx)?$": "babel-jest"
  },
  "verbose": false
}