{
  "name": "@jkr26/react-forms-builder-logic",
  "description": "Small library to simplify the use of forms in React.",
  "version": "0.1.1",
  "keywords": [
    "react form",
    "form builder",
    "react",
    "react-component",
    "form"
  ],
  "repository": ".",
  "license": "MIT",
  "main": "dist/react-form-builder.js",
  "module": "dist/react-form-builder.js",
  "jsnext:main": "dist/react-form-builder.js",
  "sideEffects": false,
  "author": "Jesus Javega <jesusgraficap@gmail.com>",
  "scripts": {
    "prebuild": "npm run clean",
    "build": "rollup -c",
    "clean": "rimraf dist",
    "lint": "eslint src",
    "prepare": "npm run prebuild && npm run build",
    "test": "BABEL_ENV=test jest  --watchAll --no-cache --config ./jest.config.json",
    "test:ci": "BABEL_ENV=test jest --no-cache --config ./jest.config.json --runInBand",
    "test:coverage": "BABEL_ENV=test jest --watchAll --coverage --no-cache --config ./jest.config.json",
    "start:example": "webpack-dev-server --config ./webpack.config.js --mode development",
    "release": "npm publish --access=public"
  },
  "peerDependencies": {
    "react": ">=0.14.0 <17.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.1.0",
    "@babel/helper-module-imports": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.1.0",
    "@babel/plugin-transform-modules-commonjs": "^7.2.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "babel-plugin-macros": "^2.2.2",
    "babel-plugin-transform-define": "^1.3.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.13",
    "cross-env": "^5.0.1",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.8.0",
    "eslint": "^5.12.1",
    "eslint-config-prettier": "^4.0.0",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.4",
    "husky": "^1.3.1",
    "jest": "^24.0.0",
    "lint-staged": "^8.1.1",
    "prettier": "1.16.1",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-hot-loader": "^4.6.5",
    "rimraf": "^2.6.1",
    "rollup": "^0.66.2",
    "rollup-plugin-babel": "^4.0.3",
    "rollup-plugin-commonjs": "^9.1.8",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-uglify": "^6.0.0",
    "webpack": "^4.29.0",
    "webpack-cli": "^3.2.1",
    "webpack-dev-server": "^3.1.14"
  },
  "files": [
    "dist"
  ],
  "lint-staged": {
    "*.js": [
      "npm run lint",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@babel/runtime": "^7.1.2",
    "prop-types": "^15.6.0"
  }
}
